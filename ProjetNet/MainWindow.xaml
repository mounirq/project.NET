<Window x:Class="ProjetNet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetNet"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="855.557" Width="880.888">
    <Grid Margin="10,4,16,382">
        <TextBlock Margin="360,12,359,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="25" Text="Welcome to TesterApp" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.015"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>

        <lvc:CartesianChart Series="{Binding WindowPlotVM.SeriesCollection}" LegendLocation="Right" Margin="-23,486,-2,-376" >
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Sales" LabelFormatter="{Binding WindowPlotVM.YFormatter}"></lvc:Axis>
            </lvc:CartesianChart.AxisY>
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Month" Labels="{Binding WindowPlotVM.Labels}"></lvc:Axis>
            </lvc:CartesianChart.AxisX>
        </lvc:CartesianChart>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="87,92,0,0" TextWrapping="Wrap" Text="Strike" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged_1"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="91,135,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="136,135,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="177,135,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <TextBlock HorizontalAlignment="Left" Margin="124,136,0,0" TextWrapping="Wrap" Text="/" VerticalAlignment="Top" Width="12" Height="22" RenderTransformOrigin="-14.633,-1.954"/>
        <TextBlock HorizontalAlignment="Left" Margin="165,136,0,0" TextWrapping="Wrap" Text="/" VerticalAlignment="Top" Width="12" Height="22" RenderTransformOrigin="-14.633,-1.954"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="91,237,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="136,237,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="177,237,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="24"/>
        <TextBlock HorizontalAlignment="Left" Margin="124,238,0,0" TextWrapping="Wrap" Text="/" VerticalAlignment="Top" Width="12" Height="22" RenderTransformOrigin="-14.633,-1.954"/>
        <TextBlock HorizontalAlignment="Left" Margin="165,238,0,0" TextWrapping="Wrap" Text="/" VerticalAlignment="Top" Width="12" Height="22" RenderTransformOrigin="-14.633,-1.954"/>
        <TextBlock Margin="3,53,770,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.015"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform><Run Text="TypeOption"/><Run Text=" :"/><Run Text=" "/><Run Text=" "/></TextBlock>
        <TextBlock Margin="6,93,771,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Text="Strike :" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.015"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <CheckBox Content="VanillaCall" HorizontalAlignment="Left" Margin="87,55,0,0" VerticalAlignment="Top"/>
        <CheckBox Content="BasketOption" HorizontalAlignment="Left" Margin="165,55,0,0" VerticalAlignment="Top"/>
        <TextBlock Margin="5,134,772,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Text="Maturity :" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.015"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock Margin="4,179,773,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Text="Underlying :" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.015"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <ComboBox ItemsSource="{Binding ComponentInfoList}" SelectedItem="{Binding UniverseVM.TopologyVM}" Margin="86,178,641,239" Text="Underlying">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Name}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
    </Grid>
</Window>
